{
  "name": "vue-tasty-burgers",
  "version": "1.2.1",
  "description": "An easy to use hamburger buttons library for VueJS",
  "author": "Fabrizio Meinero <fabrizio@imfaber.me>",
  "repository": {
    "type": "git",
    "url": "git@github.com:imfaber/vue-tasty-burgers.git"
  },
  "license": "MIT",
  "keywords": [
    "vue",
    "vuejs",
    "hamburger",
    "button"
  ],
  "dependencies": {},
  "peerDependencies": {
    "vue": "^2.5.11"
  },
  "scripts": {
    "lint": "eslint --ext .js,.vue src test/unit/specs",
    "lint:fix": "npm run lint -- --fix",
    "lint-docs-vue": "eslint --plugin vue --ext .js,.vue docs/src",
    "lint-docs-vue:fix": "npm run lint-docs-vue -- --fix",
    "lint-docs-md-tag": "eslint --plugin script-tags --ext .js,.vue,.md docs/src --rule 'eol-last':0",
    "lint-docs-md-tag:fix": "npm run lint-docs-md-tag -- --fix",
    "lint-docs": "npm run lint-docs-vue && npm run lint-docs-md-tag",
    "build": "cross-env NODE_ENV=production rollup -c",
    "dev": "rollup -c -w",
    "unit": "cross-env NODE_ENV=test jest",
    "test": "npm run unit",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "build-docs": "npm run clean-docs && cross-env NODE_ENV=production webpack --progress --hide-modules",
    "clean-docs": "rimraf docs/dist",
    "storybook": "start-storybook --port 6006"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-jsx": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "@babel/preset-stage-2": "^7.8.3",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-node-resolve": "^7.1.3",
    "@rollup/plugin-replace": "^2.3.1",
    "@storybook/vue": "^5.3.18",
    "@vue/test-utils": "^1.0.0-beta.33",
    "autoprefixer": "^8.5.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-helper-vue-jsx-merge-props": "^2.0.3",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-transform-vue-jsx": "^3.5.0",
    "babel-preset-vue": "^2.0.2",
    "cross-env": "^5.1.1",
    "css-loader": "^0.28.7",
    "eslint": "^4.19.1",
    "eslint-config-standard": "^11.0.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-script-tags": "^0.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "eslint-plugin-vue": "^4.0.0",
    "file-loader": "^1.1.6",
    "html-webpack-plugin": "^3.2.0",
    "inject-loader": "^4.0.1",
    "jest": "^25.3.0",
    "node-sass": "^4.13.1",
    "postcss-loader": "^2.0.8",
    "rimraf": "^2.6.2",
    "rollup": "^1.20.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-terser": "^5.3.0",
    "rollup-plugin-vue": "^5.1.6",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.21.0",
    "uglify-es": "^3.3.9",
    "vue": "^2.5.11",
    "vue-jest": "^3.0.5",
    "vue-loader": "^15.9.1",
    "vue-md-loader": "^1.0.0",
    "vue-router": "^3.0.1",
    "vue-style-loader": "^4.1.0",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  },
  "main": "dist/vue-tasty-burgers.js",
  "module": "dist/vue-tasty-burgers.js",
  "jsnext:main": "dist/vue-tasty-burgers.js",
  "files": [
    "dist"
  ]
}
