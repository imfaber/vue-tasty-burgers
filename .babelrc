{
  "presets": [
    ["@babel/preset-env", {
      "modules": false,
      "targets": {
        "browsers": ["> 1%", "last 2 versions", "not ie <= 8"]
      }
    }]
  ],
  "plugins": ["transform-vue-jsx", "@babel/plugin-transform-runtime"],
  "env": {
    "test": {
      "presets": [
        ["@babel/preset-env", {
          "targets": { "node": 8 }
        }]
      ],
      "plugins": ["transform-vue-jsx", "transform-es2015-modules-commonjs"]
    }
  }
}
